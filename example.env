# CVAT Configuration
# The hostname or IP address where CVAT will be accessible (e.g. 192.168.0.220, localhost)
CVAT_HOST=

# The port where CVAT will be accessible (default: 9090)
CVAT_PORT=

# CVAT version to deploy (e.g., dev, 2.35.0, latest)
CVAT_VERSION=2.35.0

# Default API username for CVAT
CVAT_USERNAME=

# Default API password for CVAT
CVAT_PASSWORD=

# Email Configuration for User Registration and Notifications
# Email address used to send emails from CVAT
EMAIL_HOST_USER=

# Email app password (not your regular password - e.g., generate from Google Account settings)
EMAIL_HOST_PASSWORD=

# SMTP server hostname (default: smtp.gmail.com for Gmail)
EMAIL_HOST=

# SMTP server port (587 for TLS, 465 for SSL, 25 for unencrypted, default: 587)
EMAIL_PORT=

# Enable TLS encryption for email (True/False, default: True)
EMAIL_USE_TLS=

# Django email backend (default: django.core.mail.backends.smtp.EmailBackend)
EMAIL_BACKEND=

# "From" address that will appear in sent emails
EMAIL_FROM=

# Django site ID for multi-site configuration (default: 3)
SITE_ID=

# ClearML Configuration
# ClearML API server URL (e.g., http://192.168.0.220:8008)
CLEARML_API_HOST=

# ClearML Web server URL (e.g., http://192.168.0.220:8080)
CLEARML_WEB_HOST=

# ClearML Files server URL (e.g., http://192.168.0.220:8081)
CLEARML_FILES_HOST=

# ClearML API access key
CLEARML_API_KEY=

# ClearML API secret key
CLEARML_API_SECRET=

# ClearML Frontend Configuration (for React UI)
# ClearML Web UI URL (e.g., http://192.168.0.220:8083)
REACT_APP_CLEARML_WEB_URL=

# ClearML API URL for frontend (e.g., http://192.168.0.220:8008)
REACT_APP_CLEARML_API_URL=

# ClearML Frontend Access Key
REACT_APP_CLEARML_ACCESS_KEY=

# ClearML Frontend Secret Key
REACT_APP_CLEARML_SECRET_KEY=
